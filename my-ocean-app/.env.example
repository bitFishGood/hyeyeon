# MongoDB 연결 URL (예: 로컬 몽고디비)
MONGO_URI=mongodb://localhost:27017/myocean

# JWT 시크릿 키 (임의의 복잡한 문자열로 변경하여 사용)
JWT_SECRET=your_jwt_secret_here

# Kakao OAuth 앱 키 및 Redirect URI (실제 Kakao 연동시 사용)
KAKAO_CLIENT_ID=your_kakao_rest_api_key
KAKAO_REDIRECT_URI=http://localhost:5000/api/auth/kakao/callback

# 서버 포트 설정 (선택사항)
PORT=5000
